        "@nuxt/fonts": "^0.12.1",
        "@nuxt/icon": "^2.1.0",
        "@nuxt/kit": "^4.2.1",
        "@nuxtjs/turnstile": "^1.1.1",
        "@prisma/adapter-pg": "^7.0.0",
        "@prisma/client": "^7.0.0",
        "@simplewebauthn/browser": "^13.2.2",
        "@simplewebauthn/types": "^12.0.0",
        "@types/fluent-ffmpeg": "^2.1.28",
        "@types/stream-chain": "^2.1.0",
        "@types/stream-json": "^1.7.8",
        "@unocss/eslint-config": "^66.5.9",
        "@unocss/nuxt": "^66.5.9",
        "@unocss/reset": "^66.5.9",
        "@vite-pwa/nuxt": "^1.0.7",
        "@vueuse/core": "^14.0.0",
        "@vueuse/nuxt": "^14.0.0",
        "apexcharts": "^5.3.6",
        "bufferutil": "^4.0.9",
        "changelogen": "^0.6.2",
        "dayjs": "^1.11.19",
        "eslint": "^9.39.1",
        "eslint-plugin-simple-import-sort": "^12.1.1",
