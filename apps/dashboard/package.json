        "@orama/stopwords": "^3.1.18",
        "@simplewebauthn/server": "^13.2.2",
        "argon2": "^0.44.0",
        "color": "^5.0.3",
        "consola": "^3.4.2",
        "defu": "^6.1.4",
        "dotenv": "^17.2.3",
        "engine.io": "^6.6.4",
        "filesize": "^11.0.13",
        "fluent-ffmpeg": "^2.1.3",
        "nanoid": "^5.1.6",
        "otplib": "^12.0.1",
        "pathe": "^2.0.3",
        "sharp": "^0.34.5",
        "socket.io": "^4.8.1",
        "stream-chain": "^3.4.0",
        "stream-json": "^1.9.1",
        "tar": "^7.5.2",
        "uqr": "^0.1.2",
        "vue": "^3.5.24",
        "vue-router": "^4.6.3",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@iconify-json/heroicons": "^1.2.3",
        "@iconify-json/lucide": "^1.2.80",
        "@iconify-json/mdi": "^1.2.3",
        "@iconify-json/solar": "^1.2.5",
