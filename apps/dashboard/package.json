{
    "name": "dashboard",
    "type": "module",
    "scripts": {
        "build": "pnpm db:generate && nuxt build",
        "dev": "pnpm db:generate && pnpm db:deploy && nuxt dev",
        "preview": "pnpm db:deploy && node --env-file-if-exists=.env -r ./scripts/fixPrisma.js .output/server/index.mjs",
        "postinstall": "nuxt prepare",
        "cleanup": "nuxt cleanup",
        "lint:types": "nuxt typecheck",
        "lint:code": "eslint --fix .",
        "lint": "pnpm lint:code && pnpm lint:types",
        "check": "pnpm build && pnpm lint",
        "release": "changelogen --release",
        "db:prototype": "prisma db push",
        "db:migrate": "prisma migrate dev --create-only",
        "db:studio": "prisma studio",
        "db:generate": "prisma generate --no-hints",
        "db:deploy": "prisma migrate deploy"
    },
    "dependencies": {
        "@ffmpeg-installer/ffmpeg": "^1.1.0",
        "@fileship/telemetry": "workspace:*",
        "@orama/orama": "^3.1.13",
        "@simplewebauthn/server": "^13.1.2",
        "@xenova/transformers": "^2.17.2",
        "argon2": "^0.44.0",
        "color": "^5.0.0",
        "consola": "^3.4.2",
        "defu": "^6.1.4",
        "engine.io": "^6.6.4",
        "filesize": "^11.0.2",
        "fluent-ffmpeg": "^2.1.3",
        "nanoid": "^5.1.5",
        "otplib": "^12.0.1",
        "pathe": "^2.0.3",
        "sharp": "^0.34.3",
        "socket.io": "^4.8.1",
        "stream-chain": "^3.4.0",
        "stream-json": "^1.9.1",
        "tar": "^7.4.3",
        "uqr": "^0.1.2",
        "vue": "^3.5.21",
        "vue-router": "^4.5.1",
        "zod": "^4.1.5"
    },
    "devDependencies": {
        "@iconify-json/heroicons": "^1.2.2",
        "@iconify-json/heroicons-solid": "^1.2.1",
        "@iconify-json/iconamoon": "^1.2.2",
        "@iconify-json/mdi": "^1.2.3",
        "@nuxt/eslint": "^1.9.0",
        "@nuxt/fonts": "^0.11.4",
        "@nuxt/icon": "^2.0.0",
        "@nuxt/kit": "^4.1.1",
        "@nuxtjs/turnstile": "^1.0.0",
        "@prisma/adapter-pg": "^6.16.2",
        "@prisma/client": "^6.16.2",
        "@simplewebauthn/browser": "^13.1.2",
        "@simplewebauthn/types": "^12.0.0",
        "@types/fluent-ffmpeg": "^2.1.27",
        "@types/stream-chain": "^2.1.0",
        "@types/stream-json": "^1.7.8",
        "@unocss/eslint-config": "^66.5.0",
        "@unocss/nuxt": "^66.5.0",
        "@unocss/reset": "^66.5.0",
        "@vite-pwa/nuxt": "^1.0.4",
        "@vueuse/core": "^13.9.0",
        "@vueuse/nuxt": "^13.9.0",
        "apexcharts": "^5.3.4",
        "bufferutil": "^4.0.9",
        "changelogen": "^0.6.2",
        "dayjs": "^1.11.18",
        "eslint": "^9.35.0",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "gsap": "^3.13.0",
