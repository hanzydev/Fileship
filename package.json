{
    "name": "fileship",
    "description": "Fileship is an open-source and self-hosted image uploading service with cool features.",
    "version": "3.2.1",
    "type": "module",
    "scripts": {
        "build:prisma": "prisma generate --no-hints",
        "build:fileship": "nuxt build",
        "build": "pnpm /^build:.*/",
        "dev": "pnpm build:prisma && nuxt dev",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare",
        "cleanup": "nuxt cleanup",
        "lint:types": "nuxt typecheck",
        "lint:code": "eslint --fix .",
        "lint:style": "prettier --write .",
        "lint": "pnpm /^lint:.*/",
        "check": "pnpm build && pnpm lint",
        "release": "changelogen --release",
        "db:prototype": "prisma db push --skip-generate && pnpm build:prisma",
        "db:migrate": "prisma migrate dev --create-only",
        "db:studio": "prisma studio"
    },
    "dependencies": {
        "@ffmpeg-installer/ffmpeg": "^1.1.0",
        "@orama/orama": "^3.1.10",
        "@simplewebauthn/server": "^13.1.1",
        "@xenova/transformers": "^2.17.2",
        "argon2": "^0.43.0",
        "color": "^5.0.0",
        "consola": "^3.4.2",
        "defu": "^6.1.4",
        "engine.io": "^6.6.4",
        "filesize": "^10.1.6",
        "fluent-ffmpeg": "^2.1.3",
        "nanoid": "^5.1.5",
        "otplib": "^12.0.1",
        "pathe": "^2.0.3",
        "sharp": "^0.34.2",
        "socket.io": "^4.8.1",
        "stream-chain": "^3.4.0",
        "stream-json": "^1.9.1",
        "tar": "^7.4.3",
        "uqr": "^0.1.2",
        "vue": "^3.5.17",
        "vue-router": "^4.5.1",
        "zod": "^3.25.73"
    },
    "devDependencies": {
        "@iconify-json/heroicons": "^1.2.2",
        "@iconify-json/heroicons-solid": "^1.2.1",
        "@iconify-json/iconamoon": "^1.2.2",
        "@iconify-json/mdi": "^1.2.3",
        "@nuxt/eslint": "^1.5.2",
        "@nuxt/fonts": "^0.11.4",
        "@nuxt/icon": "^1.15.0",
        "@nuxt/kit": "^3.17.6",
        "@nuxtjs/turnstile": "^1.0.0",
        "@prisma/client": "^6.11.1",
        "@prisma/migrate": "^6.11.1",
        "@simplewebauthn/browser": "^13.1.0",
        "@simplewebauthn/types": "^12.0.0",
        "@types/fluent-ffmpeg": "^2.1.27",
        "@types/stream-chain": "^2.1.0",
        "@types/stream-json": "^1.7.8",
        "@unocss/eslint-config": "^66.3.2",
        "@unocss/nuxt": "^66.3.2",
        "@unocss/reset": "^66.3.2",
        "@vite-pwa/nuxt": "^1.0.4",
        "@vueuse/core": "^13.5.0",
        "@vueuse/nuxt": "^13.5.0",
        "apexcharts": "^4.7.0",
        "bufferutil": "^4.0.9",
        "changelogen": "^0.6.1",
        "dayjs": "^1.11.13",
        "eslint": "^9.30.1",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "gsap": "^3.13.0",
        "highlight.js": "^11.11.1",
        "marked": "^15.0.12",
        "marked-highlight": "^2.2.2",
        "nuxt": "^3.17.6",
        "prettier": "^3.6.2",
        "prisma": "^6.11.1",
        "scule": "^1.3.0",
        "socket.io-client": "^4.8.1",
        "typescript": "^5.8.3",
        "unocss-preset-animations": "^1.2.1",
        "utf-8-validate": "^6.0.5",
        "vue-sonner": "^2.0.1",
        "vue-tsc": "^2.2.12",
        "vue3-apexcharts": "^1.8.0"
    },
    "pnpm": {
        "overrides": {
            "unconfig": "^7.3.2",
            "sharp": "0.33.4"
        },
        "patchedDependencies": {
            "@prisma/client": "patches/@prisma__client.patch"
        },
        "supportedArchitectures": {
            "os": [
                "win32",
                "linux",
                "darwin"
            ],
            "cpu": [
                "x64",
                "arm64"
            ]
        }
    },
    "packageManager": "pnpm@10.12.4"
}
